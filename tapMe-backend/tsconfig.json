{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "resolveJsonModule": true,
      "moduleResolution": "node"
    },
    "include": ["**/*"],
    "exclude": ["node_modules", "**/*.test.ts"]
  }
  